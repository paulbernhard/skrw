<%= form_with model: upload, builder: Skrw::FormBuilder, class: 's-form' do |f| %>
  
  <%= f.errors %>

  <%= f.hidden_field(:uploadable_type, value: f.object.uploadable_type) %>
  <%= f.hidden_field(:uploadable_id, value: f.object.uploadable_id) %>
  <%= f.hidden_field(:file, value: f.object.cached_file_data) %>

  <%= f.file_field(:file) %>

  <%= f.submit %>
<% end %>